<?xml version="1.0" ?>
<robot name="My Camera">
    <webots>
        <device reference="camera" type="Camera">
            <ros>
                <topicName>/camera/top_view</topicName>
                <frameName>camera_top_link</frameName>
                <scanRate>10</scanRate>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device> 
    </webots>
</robot>